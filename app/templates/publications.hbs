<h2>Publications</h2><br>

<h3>Papers</h3><br>
<table class="table">
    <thead>
        <th scope="col">Abbreviation</th>
        <th scope="col">Citation</th>
        <th scope="col">Notes</th>
    </thead>
    <tbody>
        {{#each reverse as |paper|}}
        <tr>
            <th scope="row"><em>{{paper.abbrev}}</em></th>
            <td><b>{{#link-to 'paper' paper.id}}{{paper.title}}{{/link-to}}</b>
                <br>
                {{paper.citation}}
                <br>
                {{#each paper.extras as |e|}}
                <a href="{{e.url}}">[{{e.type}}]</a>
                {{/each}}
                {{outlet}}
            </td>
            <td>{{paper.notes}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
<br>

